# Firebase Configuration
# Get these values from Firebase Console: https://console.firebase.google.com/

# API Key - Found in Project Settings
VITE_FIREBASE_API_KEY=your_api_key_here

# Auth Domain - Format: [PROJECT_ID].firebaseapp.com
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com

# Project ID
VITE_FIREBASE_PROJECT_ID=your_project_id

# Storage Bucket
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com

# Messaging Sender ID
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789

# App ID
VITE_FIREBASE_APP_ID=1:123456789:web:your_app_id

# Google Gemini API Key (Optional)
VITE_GEMINI_API_KEY=your_gemini_key_here
# Toggle to use real Firebase + Gemini services (set to 'true' to use real services)
VITE_USE_REAL_SERVICES=false
